#!/bin/sh -e

#? Dynamic Vars
# snapctl set :<slotname> slotattribute=value # to create an attribute for the slot side

#? to read attributes of the local slot
PUB_ID="$(snapctl get :ocis-configure plug-publisher-id)"

echo $PUB_ID

#? Read attrs of plug
CONTENT_ID="$(snapctl get --plug :ocis-config content)" # to read attributes of the plug

echo $CONTENT_ID

echo $(env)
